admin:
  existingSecret: ""
  user: admin         
  password: "{{ grafana_admin_password | default('changeme123') }}"

persistence:
  enabled: true
  storageClassName: ""  
  accessModes:
    - ReadWriteOnce
  size: 10Gi

datasources:
  datasources.yaml:
    apiVersion: 1
    datasources:
      - name: Prometheus
        type: prometheus
        access: proxy
        url: "{{ prometheus_datasource_url }}"
        isDefault: true
